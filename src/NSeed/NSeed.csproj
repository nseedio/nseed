<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>7.3</LangVersion>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>    
    <WarningLevel>4</WarningLevel>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <WarningsAsErrors />
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.1.0-prealpha</Version>
    <Authors>NSeed, ironcev</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/nseedio/nseed</PackageProjectUrl>
    <RepositoryUrl></RepositoryUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/nseedio/nseed/master/images/dandelion128x128.png</PackageIconUrl>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\AssemblyInfo.Common.cs" Link="Properties\AssemblyInfo.Common.cs" />
    <Compile Include="..\Common\Guards\NSeedInternalErrorArgumentException.cs" Link="Guards\NSeedInternalErrorArgumentException.cs" />
    <Compile Include="..\Common\Guards\NSeedInternalErrorArgumentNullException.cs" Link="Guards\NSeedInternalErrorArgumentNullException.cs" />
  </ItemGroup>

</Project>
